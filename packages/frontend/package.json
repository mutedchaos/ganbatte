{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.3.4",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.12",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-relay": "^11.0.2",
    "react-scripts": "4.0.3",
    "relay-runtime": "^11.0.2",
    "styled-components": "^5.3.0",
    "typescript": "^4.2.4",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run relay-compiler-watch\"",
    "relay-compiler": "relay-compiler --language typescript --extensions ts tsx",
    "relay-compiler-watch": "npm run relay-compiler -- --watch",
    "update": "cd ../server && npm run export-schema && cp lib/schema.graphql ../frontend && cd - && npm run relay-compiler",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/reach__router": "^1.3.7",
    "@types/react-relay": "^11.0.1",
    "@types/relay-runtime": "^11.0.0",
    "@types/styled-components": "^5.1.9",
    "babel-plugin-relay": "^11.0.2",
    "concurrently": "^6.0.2",
    "graphql": "^15.5.0",
    "relay-compiler": "^11.0.2",
    "relay-compiler-language-typescript": "^14.0.0",
    "relay-config": "^11.0.2",
    "watchman": "^1.0.0"
  }
}
