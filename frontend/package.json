{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.12",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "graphviz-react": "^1.2.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^17.0.2",
    "react-relay": "^14.1.0",
    "react-router-dom": "^6.4.3",
    "relay-runtime": "^14.1.0",
    "styled-components": "^5.3.0",
    "typescript": "^4.2.4",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run relay-compiler-watch\"",
    "relay-compiler": "relay-compiler",
    "relay-compiler-watch": "nodemon -x npm -- run relay-compiler",
    "update": "cd ../server && npm run export-schema && cp lib/schema.graphql ../frontend && cd - && npm run relay-compiler",
    "dev": "vite",
    "dev:docker": "concurrently \"vite --host 0.0.0.0\" \"npm run relay-compiler-watch\"",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.171",
    "@types/react-autosuggest": "^10.1.5",
    "@types/react-relay": "^14.1.2",
    "@types/relay-runtime": "^14.1.4",
    "@types/styled-components": "^5.1.9",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-plugin-relay": "^14.1.0",
    "concurrently": "^6.0.2",
    "graphql": "^15.5.0",
    "nodemon": "^2.0.20",
    "relay-compiler": "^14.1.0",
    "vite": "^3.2.2",
    "vite-plugin-relay": "^2.0.0",
    "vite-tsconfig-paths": "^3.5.2"
  }
}
